<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title %>
  <br><br><br>
  <%= f.label :artist_name %>
  <br>
  <%= f.text_field :artist_name %>
  <br><br><br>
  <%= f.label :genre_name %>
  <br>
  <%= f.collection_select :genre_id, @genres, :id, :name %>
  <br><br><br>
  <input name='song[note_contents][]' id='song_notes_1'>
  <input name='song[note_contents][]' id='song_notes_2'>
  <input name='song[note_contents][]' id='song_notes_3'>
  <br><br><br>
  <%= f.submit "commit" %>
<% end %>
